package e1reformat.util;

import java.util.Comparator;

import e1reformat.entity.enrich.CallZoneGroup;

public class ZoneGroupComparator implements Comparator<CallZoneGroup> {

    @Override
    public int compare(CallZoneGroup zone1, CallZoneGroup zone2) {

        String code1 = zone1.getCode();
        String code2 = zone2.getCode();
        if (code1 == null || code2 == null) {
            return 0;
        } else {
            try {
                return (int) (Long.valueOf(code1) - Long.valueOf(code2));
            } catch (NumberFormatException nfe) {
                return 0;
            }
        }
    }
}
