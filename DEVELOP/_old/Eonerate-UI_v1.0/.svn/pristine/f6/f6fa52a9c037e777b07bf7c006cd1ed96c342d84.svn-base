package eonerateui.gui.menu.configuration;

public class AdapterConfig {
	private String inputTable;
	private String outputTable;
	private String initStatement;
	public String getInputTable() {
		return inputTable;
	}
	public void setInputTable(String inputTable) {
		this.inputTable = inputTable;
	}
	public String getOutputTable() {
		return outputTable;
	}
	public void setOutputTable(String outputTable) {
		this.outputTable = outputTable;
	}
	public String getInitStatement() {
		return initStatement;
	}
	public void setInitStatement(String initStatement) {
		this.initStatement = initStatement;
	}
	public AdapterConfig(String inputTable, String outputTable,
			String initStatement) {
		super();
		this.inputTable = inputTable;
		this.outputTable = outputTable;
		this.initStatement = initStatement;
	}
	public AdapterConfig() {
		super();
	}

	
	
}
