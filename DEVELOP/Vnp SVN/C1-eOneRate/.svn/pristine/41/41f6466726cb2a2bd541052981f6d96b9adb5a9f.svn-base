        returnStr.append(DURATION).append(SPLITTER);
        returnStr.append(TOTAL_USAGE).append(SPLITTER);
        returnStr.append(B_NUMBER).append(SPLITTER);
        returnStr.append(ZONE).append(SPLITTER);
        returnStr.append(NW_GROUP).append(SPLITTER);
        returnStr.append(SERVICE_FEE).append(SPLITTER);
        returnStr.append(SERVICE_FEE_ID).append(SPLITTER);
        returnStr.append(CHARGE_FEE).append(SPLITTER);
        returnStr.append(CHARGE_FEE_ID).append(SPLITTER);
        returnStr.append(LAC).append(SPLITTER);
        returnStr.append(CELL_ID).append(SPLITTER);
        returnStr.append(SUBSCRIBER_UNBILL).append(SPLITTER);
        returnStr.append(BU_ID).append(SPLITTER);
        returnStr.append(OLD_BU_ID).append(SPLITTER);
        returnStr.append(FREE_COST).append(SPLITTER);
        returnStr.append(FREE_VALUE).append(SPLITTER);
        returnStr.append(FREE_AMOUNT).append(SPLITTER);
        returnStr.append(FREE_BLOCK).append(SPLITTER);
        returnStr.append(DIAL_DIGIT).append(SPLITTER);
        returnStr.append(CDR_RECORD_HEADER_ID).append(SPLITTER);
        returnStr.append(CDR_SEQUENCE_NUMBER).append(SPLITTER);
        returnStr.append(LOCATION_NO).append(SPLITTER);
        returnStr.append(MSC_ID).append(SPLITTER);
        returnStr.append(UNIT_TYPE_ID).append(SPLITTER);
        returnStr.append(PRIMARY_OFFER_ID).append(SPLITTER);
        returnStr.append(DISCOUNT_ITEM_ID).append(SPLITTER);
//        returnStr.append(BALANCE_CHANGE).append(SPLITTER);
        returnStr.append(RERATE_FLAG).append(SPLITTER);
        returnStr.append(AUT_FINAL_ID).append(SPLITTER);
        returnStr.append(TARIFF_PLAN_ID).append(SPLITTER);
        returnStr.append(ERROR_CODE).append(SPLITTER);
        returnStr.append(PAYMENT_ID).append(SPLITTER);

        return returnStr.toString();
