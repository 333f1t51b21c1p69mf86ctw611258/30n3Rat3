    @Override
    public String toString() {

        StringBuilder returnStr = new StringBuilder();

        String SPLITTER = IConstant.EXPORT_CSV.SPLITTER;
        returnStr.append(A_NUMBER).append(SPLITTER);
        returnStr.append(TYPE_OF_CDR).append(SPLITTER);
